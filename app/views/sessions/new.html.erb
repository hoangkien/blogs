<h1>Login</h1>
<%= form_for :users, url: login_path do |f| %>
  <p>
    <%= f.label :username, "User name" %>  
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password, "Password" %>
    <%= f.text_field :password %>
  </p>
  <p>
    <%= f.submit "Login", :disable_with => 'Logining...' %>
  </p>
<% end -%>
